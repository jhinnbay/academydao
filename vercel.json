{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "functions": {
    "netlify/functions/api.ts": {
      "handler": "dist/server/node-build.mjs"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
